--cleaning up the road
--a work in progress

-- global lists
players = {}
trashes = {}

-- player object
player = {}

function player:new(x,y,sprite_id)
	local o = {}
	setmetatable(o,self)
	self.__index = self
	o.x = x or 10
	o.y = y or 20
	o.dx = 0
	o.dy = 0
	o.is_looking_left = false
	o.sprite_id = sprite_id
	return o
end

function player:draw()
 spr(self.sprite_id, self.x, self.y, 1, 1, self.is_looking_left)
end


-- global init

function _init()
 add(players, player:new(10,rnd(5)+10,1))
 add(players, player:new(5,rnd(20)+20,2)) 
 for i = 1,10,1 do
  add(trashes, trash:new())
 end
end

-- update on every frame
function _update()
 handle_controllers()
end

function handle_controllers()
 for i,player in pairs(players) do
  if(btn(⬅️,i-1)) then 
   player.dx = -1
   player.is_looking_left = true
  elseif(btn(➡️,i-1)) then 
   player.dx = 1
   player.is_looking_left = false
  else player.dx = 0 end
 
  if(btn(⬆️,i-1)) then player.dy = -1
  elseif(btn(⬇️,i-1)) then player.dy = 1
  else player.dy = 0 end
  player.x += player.dx
  player.y += player.dy 
 end
end

-- draw on every frame
function _draw()
 cls()
 map()
 foreach(trashes, function(t) t:draw() end)
 foreach(players, function(p) p:draw() end)
-- draw_trash_test()
end

-- testing methods
function draw_trash_test()
 spr(3, 30, 20, 1, 1)
 spr(4, 40, 20, 1, 1)
 spr(5, 50, 20, 1, 1)
 spr(6, 60, 20, 1, 1)
 spr(7, 30, 30, 1, 1)
 spr(8, 40, 30, 1, 1)
 spr(9, 50, 30, 1, 1)
end
